{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "dahara-overlay-browser",
	"version": "0.1.0",
	"identifier": "browser.overlay.dahara.de",
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"label": "main",
				"title": "dahara-overlay-browser",
				"url": "https://dgtl.dahara.de",
				"width": 600,
				"height": 200,
				"decorations": true,
				"alwaysOnTop": true,
				"resizable": true,
				"transparent": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
	}
}
